# Copy this file to .env and fill in secrets locally (DO NOT commit .env)
# Supabase
SUPABASE_URL=
SUPABASE_KEY=
SUPABASE_SERVICE_KEY=

# App config
PORT=3000
NODE_ENV=development

# OpenAI / OpenRouter keys
# If you use OpenAI's official API, set OPENAI_API_KEY and optionally OPENAI_MODEL
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# If you use OpenRouter, set OPENROUTER_API_KEY and optionally OPENROUTER_MODEL and OPENROUTER_BASE_URL
# Default OpenRouter base URL used by the app: https://r2.openrouter.ai/v1
OPENROUTER_API_KEY=
OPENROUTER_MODEL=r1
OPENROUTER_BASE_URL=https://r2.openrouter.ai/v1

# Other optional settings
# LOG_LEVEL=debug
